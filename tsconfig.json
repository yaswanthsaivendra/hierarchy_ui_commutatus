{
    "compilerOptions": {
      "target": "ES6",
      "module": "commonjs",
      "strict": true,
      "jsx": "react",
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "moduleResolution": "node",
      "outDir": "./dist",
      "rootDir": "./src",
      "noImplicitAny": true,
      "resolveJsonModule": true
    },
    "include": ["src"],
    "exclude": ["node_modules"]
}
  